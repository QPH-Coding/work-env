      });
    } else {
      if (typeof msg === 'string') {
        self.demandOption(keys, msg);
      } else if (msg === true || typeof msg === 'undefined') {
        self.demandOption(keys);
      }
    }

    return self
  };

  self.demandCommand = function demandCommand (min, max, minMsg, maxMsg) {
