  configuration$1.throwExceptionIf(
    config,
    configuration$1.not(configuration$1.anObject(config.appenders)),
    'must have a property "appenders" of type object.'
  );
  const appenderNames = Object.keys(config.appenders);
  configuration$1.throwExceptionIf(
